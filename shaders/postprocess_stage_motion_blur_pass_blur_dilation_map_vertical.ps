#include "common.xrh"
///////////////////////////////////////////////////////////////////////////////////
uniform sampler2D s_dilation_map;
///////////////////////////////////////////////////////////////////////////////////
float4 main(float2 TexCoords : TEXCOORD0) : COLOR0
{
    float2 Result = NULL;
    
    for (int i = 0; i < 8; i++)
    {
        for (int j; j < 8; j++)
        {
            Result += tex2Dlod0(s_dilation_map, float2(i, j) * (0.05f / screen_res.xy) + TexCoords).rg;
        }
    }
    
    return float4(Result, NULL, NULL);
}
